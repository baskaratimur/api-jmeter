              <stringProp name="script">import java.net.HttpURLConnection
import java.net.URL

def discordWebhookUrl = &quot;https://discord.com/api/webhooks/1118477030925803520/1oHgtteLRCQcmS96ra_zSfD8CwNTBjfI4N-ak9w8KiroTUJqYUBsPkM-2qHXOCRcNhIA&quot;
def errorMessage = prev.getResponseMessage()

if (prev.getResponseCode() != null &amp;&amp; prev.getResponseCode().startsWith(&quot;2&quot;)) {
    log.info(&quot;Request successful&quot;)
} else {
    sendToDiscord(errorMessage)
}

void sendToDiscord(String message) {
    try {
        def url = new URL(discordWebhookUrl)
        def connection = url.openConnection() as HttpURLConnection
        connection.requestMethod = &quot;POST&quot;
        connection.setRequestProperty(&quot;Content-Type&quot;, &quot;application/json&quot;)
        connection.doOutput = true

        def payload = [
            &quot;content&quot;: message
        ]
        def payloadJson = new groovy.json.JsonBuilder(payload).toString().getBytes(&quot;utf-8&quot;)
        connection.outputStream.write(payloadJson)

        connection.connect()

        if (connection.responseCode &gt;= 200 &amp;&amp; connection.responseCode &lt; 300) {
            log.info(&quot;Notification sent to Discord successfully&quot;)
        } else {
            log.error(&quot;Failed to send notification to Discord. Response code: &quot; + connection.responseCode)
        }
    } catch (Exception e) {
        log.error(&quot;Error sending notification to Discord: &quot; + e.getMessage())
    }
}
